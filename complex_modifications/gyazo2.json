{
	"title": "control+fnでGyazo起動",
	"rules": [
		{
			"description": "control+fnでGyazo起動",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "fn",
						"modifiers":  { "mandatory": [ "control"] }
					},
					"to_if_alone": [
						{ "shell_command": "open -a 'Gyazo.app'" }
					]
				}
			]
		}
  ]
}
